{
  "version": 2,
  "builds": [
    {
      "src": "dist/spa/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/server/node-build.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/server/node-build.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/spa/index.html"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "installCommand": "pnpm install",
  "env": {
    "NODE_ENV": "production"
  },
  "analytics": {
    "providers": [
      {
        "name": "Vercel Analytics",
        "analyticsId": "vercel-analytics"
      }
    ]
  }
}
